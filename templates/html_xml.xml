      <option name="XSL_TEXT" value="true" />
    </context>
  </template>
  <template name="tpl" value="&lt;t:pageLink page=&quot;$PAGE$&quot;&gt;$END$&lt;/t:pageLink&gt;" description="tapestry5 pagelink" toReformat="true" toShortenFQNames="true">
