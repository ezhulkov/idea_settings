    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="tife" value="&lt;t:if test=&quot;$IF$&quot;&gt;&#10;    $THEN$&#10;    &lt;t:parameter name=&quot;else&quot;&gt;&#10;        $ELSE$&#10;    &lt;/t:parameter&gt;&#10;&lt;/t:if&gt;" description="tapestry5 if/else" toReformat="true" toShortenFQNames="true">
