  </template>
  <template name="talc" value="&lt;t:actionLink t:id=&quot;$ID$&quot; t:context=&quot;$CONTEXT$&quot;&gt;$END$&lt;/t:actionLink&gt;" description="tapestry5 actionlink context" toReformat="true" toShortenFQNames="true">
