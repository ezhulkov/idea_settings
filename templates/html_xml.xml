    <variable name="ELSE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="tif" value="&lt;t:if test=&quot;$IF$&quot;&gt;&#10;    $THEN$&#10;&lt;/t:if&gt;" description="tapestry5 if" toReformat="true" toShortenFQNames="true">
    <variable name="IF" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="THEN" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="tal" value="&lt;t:actionLink t:id=&quot;$ID$&quot;&gt;$END$&lt;/t:actionLink&gt;" description="tapestry5 actionlink" toReformat="true" toShortenFQNames="true">
    <variable name="ID" expression="" defaultValue="" alwaysStopAt="true" />
