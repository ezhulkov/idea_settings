    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="tplc" value="&lt;t:pageLink page=&quot;$PAGE$&quot; context=&quot;$CONTEXT$&quot;&gt;$END$&lt;/t:pageLink&gt;" description="tapestry5 pagelink context" toReformat="true" toShortenFQNames="true">
    <variable name="PAGE" expression="" defaultValue="" alwaysStopAt="true" />
